{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "module": "CommonJS",
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}